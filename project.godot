; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="SwipeAdventure"
run/main_scene="res://Scenes/GameManager/GameManager.tscn"
config/icon="res://icon.png"

[autoload]

Cards="*res://Data/Cards.gd"
InitGame="*res://Data/InitGame.gd"
Enemies="*res://Data/Enemies.gd"
Constants="*res://Data/Constants.gd"
Encounters="*res://Data/Encounters.gd"
SetGetUtils="*res://Utils/SetGetUtils.gd"
Skills="*res://Data/Skills.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
